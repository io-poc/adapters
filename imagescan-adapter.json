{
    "name": "Black Duck Image Scan Execution",
    "type": "SimpleExecutable",
    "stage": "custom",
    "status": "",
    "config": {
        "conditions": [
            "Prescription.Security.Activities.ImageScan.Enabled"
        ],
        "command": [
            "sh",
            "/var/lib/jenkins/workspace/iRecruit-RemoteSign/.io/jenkins-plugin/run-blackduck.sh"
        ],
        "optional": [
            "Blackduck.InstanceUrl",
            "Blackduck.ProjectName",
            "Blackduck.ProjectVersion",
            "Blackduck.AuthToken"
        ],
        "require": [],
        "success": {
            "Blackduck.ScanSuccessful": "true",
            "prescription.security.activities.imageScan.successful": "true"
        },
        "failure": {
            "Blackduck.ScanSuccessful": "false"
        }
    }
}
